{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
  "headers": [
      {
        "source": "**/*.@(ico|jpg|jpeg|png|gif|webp|svg|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=1209600"
          }
        ]
      }
    ]
  }
}